{
  "name": "typescript-library-starter",
  "version": "0.0.0",
  "description": "typescript library starter",
  "license": "private",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!*.map"
  ],
  "directories": {
    "test": "test",
    "dist": "dist"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc --build --force",
    "commit": "cz",
    "fix": "pnpm run format && pnpm run lint:fix",
    "format": "syncpack format && prettier --write **/src/**/*.ts",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint src/**/*.ts test/**/*.ts --fix",
    "postinstall": "typesync",
    "reset": "./scripts/reset.sh",
    "test": "jest --no-cache --logHeapUsage",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "watch": "pnpm run build && tsc --watch"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/eslint": "^8.4.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.33",
    "@types/prettier": "^2.6.1",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "auto-changelog": "^2.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "syncpack": "^7.2.1",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.4.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.4",
    "typesync": "^0.9.1"
  }
}